# Edge Cases and Boundary Conditions Test

# Empty values
set empty_string ""
set zero_num 0
print empty_string
print zero_num

# Special characters in strings
set special "!@#$%^&*()[]{}|;:,.<>?"
print special

# Very long variable names
set very_long_variable_name_that_tests_identifier_limits "long name test"
print very_long_variable_name_that_tests_identifier_limits

# Numbers at boundaries
set max_int 2147483647
set min_int -2147483648
set decimal 3.14159265359
print max_int
print min_int
print decimal

# Function with no parameters
function simple
    return "no params"

set result simple
print result

# Nested function calls
function add x y
    return x + y

function multiply x y
    return x * y

set complex multiply (add 2 3) 4
print complex