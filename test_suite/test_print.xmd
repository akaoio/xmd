# Print Statement Test Suite for XMD Runtime
# Tests various print capabilities

# Basic print
print "Basic string print"

# Print with concatenation
set msg1 "Hello"
set msg2 "World"
print msg1 + " " + msg2

# Print with numbers
set x 10
set y 20
print "X is: " + x
print "Y is: " + y
print "X + Y = " + (x + y)

# Multi-line prints
print "Line 1"
print "Line 2"
print "Line 3"

# Empty print
print ""

# Special characters
print "Special: !@#$%^&*()"
print "Quotes: 'single' and \"double\""
print "Path: /home/user/file.txt"