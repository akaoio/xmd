# Array Indexing Test Suite
# Testing the newly implemented array access feature

set arr 10, 20, 30, 40, 50

print "Testing array indexing:"
print "arr[0] should be 10: " + arr[0]
print "arr[1] should be 20: " + arr[1]  
print "arr[2] should be 30: " + arr[2]
print "arr[3] should be 40: " + arr[3]
print "arr[4] should be 50: " + arr[4]

# Test with string array
set names "Alice", "Bob", "Charlie"
print "names[0] should be Alice: " + names[0]
print "names[1] should be Bob: " + names[1]
print "names[2] should be Charlie: " + names[2]