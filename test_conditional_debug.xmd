# CONDITIONAL DEBUG TEST
set x 10
set y 20

print "Test 1: Basic if"
if x = 10
    print "x is 10"

print "Test 2: If-else"
if y = 10
    print "y is 10"
else
    print "y is not 10"

print "Test 3: Multiple ifs"
if x = 10
    print "First: x is 10"

if y = 20
    print "Second: y is 20"

print "All tests done"