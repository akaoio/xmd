set score 75
print "Testing conditionals..."

if score > 90 then print "Grade: A"
if score > 80 then print "Grade: B"
if score > 70 then print "Grade: C"
if score > 60 then print "Grade: D"

set name "Alice"
if name = "Alice" then print "Hello, Alice!"
if name = "Bob" then print "Hello, Bob!"

set active 1
if active then print "User is active"

set empty ""
if empty then print "This should not print"