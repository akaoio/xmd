function greet name
    return "Hello " + name

function add a b
    return a + b

set result greet "World"
print result

set sum add 5 3
print "5 + 3 = " + sum

set items "apple", "banana", "cherry"
for item in items
    print "Fruit: " + item

set config
    debug true,
    retries 3,
    timeout 1000,

print "Config: " + config