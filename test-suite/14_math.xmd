// Test 14: Math Operations

// Basic arithmetic
set a 10
set b 5

print "Addition: " + (a + b)
print "Subtraction: " + (a - b)
print "Multiplication: " + (a * b)
print "Division: " + (a / b)
print "Modulo: " + (a % b)

// Complex expressions
set x 2 + 3 * 4
print "2 + 3 * 4 = " + x

set y (2 + 3) * 4
print "(2 + 3) * 4 = " + y

// Comparison operations
print "10 > 5: " + (10 > 5)
print "10 < 5: " + (10 < 5)
print "10 = 10: " + (10 = 10)
print "10 != 5: " + (10 != 5)
print "10 >= 10: " + (10 >= 10)
print "10 <= 5: " + (10 <= 5)