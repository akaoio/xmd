// Test 5: Conditionals

set score 85

// Simple if-then on one line
if score = 100 then print "Perfect!"

// If-elif-else block
if score >= 90
    print "Excellent"
elif score >= 80
    print "Good"
elif score >= 70
    print "Average"
else
    print "Try again"

// Test equality
set x 10
if x = 10
    print "x equals 10"

// Test inequality
if x != 5
    print "x not equals 5"
    
// Test comparison operators
set age 18
if age >= 18
    print "Adult"
else
    print "Minor"
    
// Ternary operator
set result age >= 18 ? "adult" : "minor"
print result