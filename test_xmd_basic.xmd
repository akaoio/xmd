set name "XMD Language"
set version "1.0.0"
set active true
set pi 3.14

print "Hello from " + name + " v" + version
print "Active: " + active
print "Pi value: " + pi

set numbers 1, 2, 3, 4, 5
print "Numbers: " + numbers

set user name "Alice", age 30, active true
print "User: " + user

if active = true then print "System is active!"
if pi > 3 then print "Pi is greater than 3"

for i in 1 .. 3
    print "Count: " + i